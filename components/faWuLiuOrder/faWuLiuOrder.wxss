.window {
  width: 90%;
  background: rgba(255,255,255,1);
  border-radius: 12rpx;
  box-sizing: border-box;
  position: fixed;
  z-index: 100;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  -webkit-transform: translate(-50%,-50%);
}
.window .window_top {
  height: 96rpx;
  border-bottom: 2rpx solid #E6E6E6; /*no*/
}
.window .item {
  height: 100rpx;
  padding: 0 30rpx;
}
.window .divide {
  margin-left: 30rpx;
  border-bottom: 2rpx solid #E6E6E6; /*no*/
}
.window .button {
  width: 400rpx;
  height: 88rpx;
  background: rgba(30,176,2,1);
  border-radius: 44rpx;
  margin: 40rpx auto
}
.window .close {
  position: absolute;
  left: 50%;
  bottom: -90rpx;
  width: 66rpx;
  height: 66rpx;
  transform: translateX(-50%)
}
.window .img1 {
  width: 42rpx;
  height: 48rpx
}
.window .img2 {
  width: 32rpx;
  height: 32rpx
}
.goods-button {
    width: 25%;
  }

  .goods-button .goods-button-item {
    border-radius: 10rpx;
    border: 2rpx solid;
    width: 90%;
    height: 60rpx;
    margin-bottom: 16rpx;
  }
.window-body{
    padding: 0 20rpx;
  }
.window-title {
    height: 100rpx;
    padding: 0 30rpx;
    box-sizing: border-box;
  }
.module {
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 13;
  }
.mask {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 12;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.4);
  }
.footer {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    height:132rpx;
    background:rgba(255,255,255,1);
    box-shadow:0 -4rpx 20rpx 0 rgba(0,0,0,0.05);
    padding: 0 30rpx;
    box-sizing: border-box;
  }
  .footer .button {
    width: 268rpx;
    height: 92rpx;
    border-radius: 46rpx;
  }
.select {
    width: 36rpx;
    height: 36rpx;
  }
.fa_input {
    text-align: right;
    border-radius: 0;
  }
.goods-selected {
    width: 150rpx;
    padding-left: 20rpx;
  }
.info-item {
    height: 100rpx;
    padding: 0 30rpx;
  }
.info_title {
    margin-top: 60rpx;
  }
.order_body {
    background:rgba(255,255,255,1);
    box-shadow:0 4rpx 20rpx 0 rgba(0,0,0,0.05);
    border-radius: 16rpx;
  }
/*路线小点*/
.addressTopDiv2 {
    padding: 20rpx 0 20rpx 30rpx;
    font-size: 2rpx;
    }
.addressDetail2 {
    margin-left: 20rpx;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    line-height: 34rpx;
    }
.img_map {
    width: 60rpx;
    height: 68rpx;
    padding-right: 30rpx;
    margin-left: 30rpx;
    }