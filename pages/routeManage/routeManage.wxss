 page{
   height: 100%
 }
.addressTopDivRoute {
  background-color: white;
  padding: 20rpx 50rpx;
  font-size: 28rpx;
}
.dotContRoute {
  width: 40rpx;
}
.greendot {
  border-radius: 50%;
  background-color: #2fb301;
  width: 24rpx;
  height: 24rpx;
}
.graydot {
  border-radius: 50%;
  background-color: rgb(219, 219, 219);
  width: 20rpx;
  height: 20rpx;
}

.reddot {
  border-radius: 50%;
  background-color: #e6454a;
  width: 24rpx;
  height: 24rpx;
}

.line {
  height: 66rpx;
  position: absolute;
  left: 18rpx;
  top: -72rpx;
  border-right: 2rpx solid #dbdbdb;
}
.line-item {
  height: 100rpx;
}
.addressDetail {
  margin-left: 20rpx;
  -webkit-line-clamp: 2;
  /*! autoprefixer: off */
  -webkit-box-orient: vertical;
  /* autoprefixer: on */
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  line-height: 38rpx;
}
._h{
  height: 96rpx;
}
.white {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 96rpx;
}

.white .white-button {
  height: 100%;
  background: white;
  color: #2fb301;
}
.map_img {
  margin-top: 323rpx;
  width:133rpx;
  height:115rpx
}