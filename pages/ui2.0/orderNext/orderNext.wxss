  .common_module {
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 15;
    background:rgba(255,255,255,1);
    border-radius:24rpx 24rpx 0 0;
    box-sizing: border-box;
  }
  .mask {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 12;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.4);
  }
  .mask2{
    position: fixed;
    top: 0;
    left: 0;
    z-index: 13;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.4);
  }
  .mask2-window{
    background-color: white;
    border-radius: 20rpx;
    position: fixed;
    z-index: 16;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    width: 80%;
  }
