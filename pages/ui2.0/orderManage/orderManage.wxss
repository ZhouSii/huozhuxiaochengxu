page {
  height: 100%;
}
.tab {
  height: 84rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 2rpx 8rpx 0px rgba(0, 0, 0, 0.04);
}

.slider {
  width: 30rpx;
  height: 4rpx;
  background: rgba(30, 175, 1, 1);
  position: absolute;
  bottom: 0;
  left: 0;
}
.content {
  padding-top: 20rpx;
  background:rgba(252,252,252,1);
}
.content_item {
  margin:0 30rpx 20rpx 30rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 4rpx 20rpx 0px rgba(0,0,0,0.05);
  border-radius:16rpx;
  box-sizing: border-box;
}
.content_item .top {
padding: 24rpx;
box-sizing: border-box;
}
.content_item  .body {
  padding: 30rpx 24rpx 30rpx 0;
  box-sizing: border-box;
}
.content_item  .footer {
  padding: 20rpx 24rpx;
  box-sizing: border-box;
}
.content_item  .time {
  width:56rpx;
  height:32rpx;
  background:rgba(232,247,229,1);
  border-radius:4rpx;  
  font-weight:400;
  color:rgba(30,175,1,1);
}
.content_item  .divide {
  height:2rpx;
  margin-left: 24rpx;
  background-color: #E6E6E6;
}
.l_w_60 {
  width: 60rpx;
}
.phone {
  width: 36rpx;
  height: 36rpx;
}
.select {
  width: 20rpx;
  height: 20rpx;
}
.cricle {
  width:52rpx;
  height:52rpx;
  border:1rpx solid rgba(230,230,230,1);
  border-radius: 52rpx;
  background-color: #E6E6E6;
}

.button_a9 {
  min-width: 128rpx;
  height:48rpx;
  border-radius:24rpx;
  border:2rpx solid rgba(169,169,169,1);
}

.button_g {
  min-width: 128rpx;
  height:48rpx;
  border-radius:24rpx;
  border:2rpx solid rgba(30,175,1,0.6);
}

.nav{
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}
.nav-title{
  width: 100%;
  height: 45px;
  line-height: 45px;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
}
.back{
  width: 48rpx;
  height: 48rpx;
  padding-left:15px;
}
.triangle {
  width: 16rpx;
  height: 12rpx;
}